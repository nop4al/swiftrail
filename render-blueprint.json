{
  "services": [
    {
      "type": "web",
      "name": "swiftrail-api",
      "runtime": "php",
      "plan": "free",
      "buildCommand": "bash build.sh",
      "startCommand": "vendor/bin/heroku-php-apache2 public/",
      "envVars": [
        {
          "key": "APP_NAME",
          "value": "Swiftrail"
        },
        {
          "key": "APP_ENV",
          "value": "production"
        },
        {
          "key": "APP_DEBUG",
          "value": "false"
        },
        {
          "key": "LOG_CHANNEL",
          "value": "stderr"
        },
        {
          "key": "DB_CONNECTION",
          "value": "pgsql"
        },
        {
          "key": "APP_KEY",
          "generateValue": true
        }
      ]
    },
    {
      "type": "pserv",
      "name": "swiftrail-db",
      "plan": "free",
      "dbName": "swiftrail",
      "user": "postgres"
    }
  ]
}
